{
    "name": "kazemisoroush/ntm",
    "description": "Network topology mapper with Laravel 5.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Soroush Kazemi",
            "email": "kazemi.soroush@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "symfony/process": "~2.0|~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4",
        "orchestra/testbench": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Ntcm\\Ntm\\": "src/",
            "Ntcm\\NtmTest\\": "tests/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
